                        <option value="Select">Select</option>
                        <option value="Yes">Yes</option>
                        <option value="No">No</option>
                     </select>
                  </div>
               </div>
            </div>
            </div>
            <br>
            <h3 align="center">
               Internship Information
            </h3>
